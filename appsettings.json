{
  "Serilog": {
    "Using": ["Serilog.Sinks.Console", "Serilog.Sinks.Seq"],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://104.199.226.207:5342" }
      }
    ],
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId"]
  },
  "PersistenceOptions": {
    "PostgreSqlOptions": {
      "ConnectionString": "Host=104.199.226.207;Port=5432;Database=richillcapital;Username=postgres;Password=postgres"
    },
    "InfluxDbOptions": {
      "ConnectionString": "http://104.199.226.207:8086"
    }
  },
  "CachingOptions": {
    "RedisOptions": {
      "ConnectionString": "localhost:6379",
      "InstanceName": "richillcapital"
    }
  },
  "DataFeedOptions": {
    "ConnectOnAppStart": true
  }
}
